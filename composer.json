{
  "name": "touchdesign/logrotate",
  "authors": [
    {
      "name": "Christin Gruber"
    }
  ],
  "license": "proprietary",
  "homepage": "https://github.com/touchdesign/logrotate",
  "keywords": [],
  "description": "Logrotate package",
  "require": {
    "php": "^7.2",
    "symfony/filesystem": "^5.1",
    "symfony/finder": "^5.1",
    "symfony/monolog-bundle": "^3.6",
    "monolog/monolog": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Touchdesign\\Logrotate\\": "src/"
    }
  },
  "autoload-dev":{
    "psr-4": {
      "Touchdesign\\Logrotate\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1",
    "phpunit/phpunit": "^8.5",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "tests": "./vendor/bin/phpunit tests",
    "lint:php": "phpcs --standard=phpcs.xml *",
    "fix:php": "phpcbf --standard=phpcs.xml *"
  }
}
